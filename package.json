{
	"name": "htmlcreator",
	"publisher": "AlanSanter",
	"displayName": "HtmlCreator",
	"description": "Automatically create files and directories for an HTML / CSS / JS project",
	"author": {
		"name": "Alan Santer"
	},
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": ["Programming Languages", "Other"],
	"icon": "icon.png",
	"activationEvents": ["onCommand:htmlcreator.html"],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "htmlcreator.html",
				"title": "Create HTML directories and files"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"bugs": {
		"url": "https://github.com/CitarOff/HTMLCreator-VSCode/issues",
		"email": "cdeveloppement38@gmail.com"
	},
	"repository": {
	  "type": "git",
	  "url": "https://github.com/CitarOff/HTMLCreator-VSCode.git"
	},
	"homepage": "https://github.com/CitarOff/HTMLCreator-VSCode/blob/main/README.md"
}
